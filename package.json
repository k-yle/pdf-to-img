{
  "name": "pdf-to-img",
  "version": "1.2.0",
  "author": "Kyle Hensel",
  "description": "ðŸ“ƒðŸ“¸ Converts PDFs to images in nodejs with no native dependencies",
  "main": "dist",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "pdf",
    "pdfjs",
    "pdf-to-img",
    "pdf-to-png",
    "jest"
  ],
  "repository": "https://github.com/k-yle/pdf-to-img",
  "scripts": {
    "build": "tsc && rm -rf dist && mv temp/src dist && rm -rf temp",
    "lint": "eslint --format pretty --ext .ts --ignore-path .gitignore .",
    "test": "jest",
    "trypublish": "npm publish || true"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "canvas": "2.10.0",
    "pdfjs-dist": "2.12.313"
  },
  "devDependencies": {
    "@types/jest-image-snapshot": "4.3.1",
    "@types/pdfjs-dist": "2.10.377",
    "eslint": "8.15.0",
    "eslint-config-kyle": "8.23.2",
    "jest": "27.5.1",
    "jest-image-snapshot": "4.5.1",
    "ts-jest": "27.1.4",
    "typescript": "4.6.4"
  },
  "eslintConfig": {
    "extends": "kyle"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./tests/setupTests.ts"
    ],
    "reporters": [
      "default",
      "jest-image-snapshot/src/outdated-snapshot-reporter.js"
    ],
    "collectCoverage": true
  }
}
