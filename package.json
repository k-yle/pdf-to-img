{
  "name": "pdf-to-img",
  "version": "2.1.2",
  "author": "Kyle Hensel",
  "description": "ðŸ“ƒðŸ“¸ Converts PDFs to images in nodejs",
  "main": "dist",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "pdf",
    "pdfjs",
    "pdf-to-img",
    "pdf-to-png",
    "jest"
  ],
  "repository": "https://github.com/k-yle/pdf-to-img",
  "scripts": {
    "build": "tsc && rm -rf dist && mv temp/src dist && rm -rf temp",
    "lint": "eslint --ignore-path .gitignore .",
    "test": "jest",
    "trypublish": "npm publish || true"
  },
  "bin": {
    "pdf2img": "./bin/cli.mjs"
  },
  "engines": {
    "node": ">=16.17"
  },
  "dependencies": {
    "canvas": "2.11.2",
    "pdfjs-dist": "3.2.146"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/jest-image-snapshot": "^6.2.1",
    "@types/node": "^20.8.6",
    "@types/pdfjs-dist": "2.10.377",
    "eslint": "^8.50.0",
    "eslint-config-kyle": "^14.12.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-image-snapshot": "^6.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./tests/setupTests.ts"
    ],
    "reporters": [
      "default",
      "jest-image-snapshot/src/outdated-snapshot-reporter.js"
    ],
    "collectCoverage": true
  }
}
